/ navbar
%header.navbar.navbar-inverse{role: "banner"}
  .navbar-header
    %button#menu-toggler.navbar-toggle{"data-toggle" => "collapse", type: "button"}
      %span.sr-only Toggle navigation
      %span.icon-bar
      %span.icon-bar
      %span.icon-bar
    %a.navbar-brand{href: "/"}
      %i.fa.fa-ambulance
      = t :brand
      / <img src="assets/img/logo.png" alt="logo">


  %ul.nav.navbar-nav.pull-right.hidden-xs

    /Profile dropdown else sign in
    - if signed_in?
      %li.settings.dropdown
        %a.dropdown-toggle.hidden-xs.hidden-sm{"data-toggle" => "dropdown", href: "#"}
          %i.fa.fa-user
            %span Account
            %b.caret
        %ul.dropdown-menu
          %li
            = link_to "Profile", current_user
          %li
            = link_to "Settings", "#"
          %li.devider
          %li
            = link_to "Sign out", signout_path, method: "delete"
    - else
      /Sign in
      %li.settings.hidden-xs.hidden-sm
        = link_to signin_path, "data-role" => "button" do
          %i.fa.fa-sign-in
            %span= t :signin

      /Sign up
      %li.settings.hidden-xs.hidden-sm
        = link_to signup_path, "data-role" => "button" do
          %i.fa.fa-plus-circle
            %span= t :signup

